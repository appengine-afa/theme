{
    "name": "appengine-afa/theme",
    "description": "This is an overried of the teepluss/laralve-theme package, as the latest version of that package does not support Laravel >=5.4",
    "keywords": ["laravel", "theme", "layout", "asset", "widget", "twig"],
    "homepage": "https://github.com/appengine-afa/theme",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "AFA",
            "email": "appengine@americaforanimals.org"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "twig/twig": "1.x"
    },
    "require-dev": {
        "illuminate/support": ">=5.0.1"
    },
    "autoload": {
        "psr-4": {
            "AppEngineAFA\\Theme\\": "src/"
        },
        "classmap": [
            "src/Exceptions.php"
        ],
        "files": [
            "src/helpers.php"
        ]
    },
    "minimum-stability": "dev"
}
